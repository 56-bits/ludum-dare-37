        <arguments>
          <argument>
            <kind>1</kind>
            <string>///main
xbor = window_get_width();
ybor = window_get_height();

mx = window_mouse_get_x();
my = window_mouse_get_y();

if is_crafting
{
    if selected == noone
    {
        if mouse_check_button_pressed(mb_left)
        {
            if (mx &lt; (xbor - 52 - 84 * num_craft)) || (mx &gt; (xbor - 32 )) || (my &lt; (ybor - 168)) || (my &gt; (ybor - 32))
            {
                is_crafting = false;
            }
        }
    }
    else
    {
        
        if !craftables[# CRAFT_ABLE, selected]
        {
            selected = noone
        }
    
        if mouse_check_button_pressed(mb_left)
        {

            if craftables[# CRAFT_ABLE, selected] &amp;&amp; placeable(selected, mouse_x, mouse_y)
            {
                if placeable(selected, mouse_x, mouse_y) &amp;&amp; (mx &lt; (xbor - 52 - 84 * num_craft)) || (mx &gt; (xbor - 32 )) || (my &lt; (ybor - 168)) || (my &gt; (ybor - 32))
                {
                    var res = craftables[# CRAFT_RES, selected]
                    
                    if ds_map_exists(res, "wood")
                    {
                        global.wood -= res[? "wood"]
                    }
                    
                    if ds_map_exists(res, "metal")
                    {
                        global.metal -= res[? "metal"]
                    }

                    instance_create( mouse_x div 32 * 32, mouse_y div 24 * 24 - 24, craftables[# CRAFT_OBJ, selected])
                    
                }
            }
        }
        
        if mouse_check_button_pressed(mb_right)
        {
            selected = noone
        }
        
    }

    if my &gt; ybor - 144 &amp;&amp; my &lt; ybor - 52
    {
        relx = mx - (xbor - 52 - 84 * num_craft)
        temp_hovered = relx div 84
        temp_hovered_rem = relx mod 84
        
        if temp_hovered &lt; num_craft &amp;&amp; temp_hovered_rem &gt; 12
        {
            hovered = temp_hovered
        }
        else
        {
            hovered = noone
        }
    }
    else
    {
        hovered = noone
    }
    
    if hovered != noone &amp;&amp; mouse_check_button_pressed(mb_left) &amp;&amp; craftables[# CRAFT_ABLE, hovered]
    {
        if selected != hovered
        {
            selected = hovered
        }
        else
        {
            selected = noone
        }
    }
    
}
else
{
    if mouse_check_button_pressed(mb_left)
    {
        if (mx &gt; (xbor - 48 - sprite_get_width(spr_crafting)/2)) &amp;&amp; (mx &lt; (xbor - 48 + sprite_get_width(spr_crafting)/2))
        {
            if (my &gt; (ybor - 48 - sprite_get_height(spr_crafting)/2)) &amp;&amp; (my &lt; (ybor - 48 + sprite_get_height(spr_crafting)/2))
            {
                is_crafting = true;
            }
        }
    }
}
</string>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>///setting craftabilty

for (i = 0; i &lt; num_craft; i++)
{
    var res = craftables[# CRAFT_RES, i]
    
    var wood_req = 0;
    var metal_req = 0;
    
    if ds_map_exists(res, "wood")
    {
        wood_req = res[? "wood"]
    }
    
    if ds_map_exists(res, "metal")
    {
        metal_req = res[? "metal"]
    }
    
    if global.wood &lt; wood_req || global.metal &lt; metal_req
    {
        craftables[# CRAFT_ABLE, i] = false
    }
    else
    {
        craftables[# CRAFT_ABLE, i] = true
    }
}
</string>

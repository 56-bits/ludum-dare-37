ybor = window_get_height();

if is_crafting
{
    draw_set_colour(c_dkgray)
    
    draw_rectangle(xbor - 52 - 84 * num_craft, ybor - 168, xbor - 32, ybor - 32, false)
    
    draw_set_colour(c_gray)
    
    draw_rectangle(xbor - 48 - 84 * num_craft, ybor - 164, xbor - 36, ybor - 36, false)
    
    for (i = num_craft - 1; i &gt;= 0; i--)
    {
        spr = craftables[# CRAFT_SPR, i]
        
        if craftables[# CRAFT_ABLE, i]
        {
            draw_sprite_ext(spr, 0, xbor - 84 * num_craft + 84 * i - 36, ybor - 144, 2, 2, 0, c_white, 1)
        }
        else
        {
            draw_sprite_ext(spr, 0, xbor - 84 * num_craft + 84 * i - 36, ybor - 144, 2, 2, 0, c_dkgray, 0.7)
        }
    }
    
    if hovered != noone
    {
        crafting_info( xbor - 84 * num_craft + 84 * hovered, ybor - 174, hovered)
    }
    
}
else
{
    draw_sprite(spr_crafting, 0, xbor - 48, ybor - 48)
}

